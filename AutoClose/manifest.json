{
    "name": "AutoCloseTab",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Tab-groups will be fixed in defaultï¼Œthis extension can auto close pages when you close browser",
    "default_locale": "zh_CN",
    "permissions": [
        "tabs", "tabGroups", "activeTab",  "storage", "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "256": "icons/icon.png"
    }
}